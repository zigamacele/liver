{
  "name": "Liver",
  "description": "Check if your favorite VTuber is live!",
  "version": "0.1.5",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_title": "Liver",
    "default_popup": "index.html",
    "default_icon": {
      "16": "public/icons/icon16.png",
      "32": "public/icons/icon32.png",
      "48": "public/icons/icon48.png"
    }
  },
  "icons": {
    "16": "public/icons/icon16.png",
    "32": "public/icons/icon32.png",
    "48": "public/icons/icon48.png",
    "128": "public/icons/icon128.png"
  }
}
